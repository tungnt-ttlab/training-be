{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "buildCommand": "npm run build",
    "devCommand": "npm start",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
        }
    ]
}
